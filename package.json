    "@mui/icons-material": "5.15.15",
    "@mui/material": "5.15.15",
    "@mui/x-date-pickers": "7.2.0",
    "@vitejs/plugin-legacy": "5.3.2",
    "@vitejs/plugin-react-swc": "3.6.0",
    "apollo-upload-client": "17.0.0",
    "dayjs": "1.11.10",
    "file-selector": "0.6.0",
    "graphql": "16.8.1",
